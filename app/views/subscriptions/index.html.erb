
<h1>Subscriptions</h1>

<table class="subscriptions--list">
  <tr>
    <th>Name</th>
    <th>Price</th>
    <th>Date</th>
    <th>Url</th>
    <th>Interval</th>
    <th>Active</th>
    <th></th>
  </tr>
  <% @subscriptions.each do |subscription| %>
    <tr>
      <td>
        <%= link_to subscription.title, subscription %>
      </td>
      <td>
        <%= subscription.price %>
      </td>
      <td>
        <%= subscription.date %>
      </td>
      <td>
        <%= subscription.url %>
      </td>
      <td>
        <%= subscription.interval %>
      </td>
      <td>
        <%= subscription.active %>
      </td>
      <td>
        <%= link_to "Edit", edit_subscription_path(subscription) %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to "New Subscription", new_subscription_path %>